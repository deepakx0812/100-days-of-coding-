#include <stdio.h>

int main() {
    int n;
    printf("Enter the size of array: ");
    scanf("%d", &n);

    int arr[n];
    int visited[n+1];  
    for (int i = 0; i <= n; i++)
        visited[i] = 0;

    printf("Enter %d elements:\n", n);

    int repeated = -1;

    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);

        if (visited[arr[i]] == 1) {
            repeated = arr[i];
        } else {
            visited[arr[i]] = 1;
        }
    }

    printf("Repeated element: %d\n", repeated);

    return 0;
}
