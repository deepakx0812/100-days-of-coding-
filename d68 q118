#include <stdio.h>

int main() {
    int n;
    printf("Enter n: ");
    scanf("%d", &n);

    int arr[n];
    int sum = 0, expected_sum;

    printf("Enter %d elements (numbers between 0 to %d, one missing):\n", n, n);
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }

    expected_sum = n * (n + 1) / 2;

    printf("The missing number is: %d\n", expected_sum - sum);

    return 0;
}

